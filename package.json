    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/esm/index.d.ts",
    "files": [
        "lib/",
        "README.md",
        "LICENSE.md"
    ],
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint"
        ]
    }
