    "version": "0.0.1",
    "author": "Ross Reicks",
    "contributors": [
        "Ramesh Nair <ram@hiddentao.com> (http://www.hiddentao.com/)",
        "Sergej Brjuchanov <serges@seznam.cz>",
        "Ross Reicks <rreicks13@gmail.com>"
    ],
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "^17.2.0",
        "@commitlint/config-conventional": "^17.2.0",
        "@types/node": "^18.8.3",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "@vitest/coverage-c8": "^0.25.2",
        "benchmark": "^2.1.4",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.0",
        "knex": "^2.3.0",
        "lint-staged": "^13.0.3",
        "lodash": "^4.17.21",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "sinon": "1.6.x",
        "typescript": "^4.8.4",
        "vitest": "^0.25.1"
    },
    "keywords": [
        "sql",
        "database",
        "rdbms",
        "typescript"
    ],
    "scripts": {
        "clean": "rimraf type-definitions && rimraf lib",
        "test": "vitest --coverage",
        "test-performance": "node performance/select.js",
        "build": "node build.js",
        "prepublish": "npm run build",
        "lint": "eslint --ext .ts,.js ./src ./test",
        "lint:fix": "yarn lint --fix",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rossreicks/squel-ts.git"
